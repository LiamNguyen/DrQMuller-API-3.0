    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "fs": "^0.0.1-security",
    "helmet": "^3.12.0",
    "http-status-codes": "^1.3.0",
    "https": "^1.0.0",
    "lodash": "^4.17.10",
    "magicli": "^0.1.2",
    "mailgun-js": "^0.18.0",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "mongoose-guid": "^1.0.31",
    "node": "^10.0.0",
    "nodemon": "^1.17.4",
    "npm-check-updates": "^2.14.2",
